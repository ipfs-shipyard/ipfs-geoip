    "p-memoize": "^7.1.0"
  },
  "devDependencies": {
    "@ipld/car": "^4.1.5",
    "aegir": "^37.5.4",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "csv-parse": "^5.3.0",
    "esmock": "^2.0.6",
    "gauge": "^4.0.4",
    "ipfs-http-client": "^58.0.1",
    "multihashes": "^4.0.3",
    "pre-commit": "^1.2.2",
    "uint8arrays": "^3.1.0"
  },
  "browser": {
